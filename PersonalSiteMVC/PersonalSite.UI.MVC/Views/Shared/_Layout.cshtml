<html>
<head>
    <title>@ViewBag.Title</title>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <link rel="stylesheet" href="~/Content/assets/css/main.css" />
    <link href="~/Content/assets/css/Custom.css" rel="stylesheet" />
</head>
<body>

    <!-- Header -->
    <header id="header">
        <a href="@Url.Action("Index","Home")" class="logo"><strong>Bryan</strong> does Code</a>
        <nav>
            <a href="#menu">Menu</a>
        </nav>
    </header>

    <!-- Nav -->
    <nav id="menu">
        <ul class="links">
            <li><a href="@Url.Action("Index","Home")">Home</a></li>            
            <li><a href="@Url.Action("Portfolio","Home")">Portfolio</a></li>
            <li><a href="@Url.Action("Links","Home")">Classmates</a></li>
            <li><a href="@Url.Action("Resume","Home")">Resume</a></li>
            <li><a href="@Url.Action("Contact","Home")">Contact</a></li>
        </ul>
    </nav>
    @RenderBody()
   
    <footer id="footer">
        <ul class="icons">

            <li><a href="#" class="icon fa-linkedin-square"><span class="label">LinkedIn</span></a></li>

        </ul>
        <div class="copyright">
            &copy; Untitled. Design: <a href="https://templated.co">TEMPLATED</a>. Images: <a href="https://unsplash.com">Unsplash</a>.
        </div>
    </footer>

    <!-- Scripts -->
    @RenderSection("scripts", required: false)
    <script src="~/Content/assets/js/jquery.min.js"></script>
    <script src="~/Content/assets/js/jquery.scrolly.min.js"></script>
    <script src="~/Content/assets/js/skel.min.js"></script>
    <script src="~/Content/assets/js/util.js"></script>
    <script src="~/Content/assets/js/main.js"></script>

</body>
</html>